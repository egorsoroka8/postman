					"var PetStatus = [\"available\", \"pending\", \"sold\"] // create array 'PetStatus'",
					"pm.environment.set(\"$randomPetStatus\", PetStatus[Math.floor(Math.random() * PetStatus.length)]) // set $randomPetStatus as any value from PetStatus array",
